<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>2022.8.0</PackageVersion>
        <Version>2022.8.0</Version>
        <Authors>Mutuduxf</Authors>
        <Company>Mutuduxf</Company>
        <PackageTags>Zaaby;MicroService;Http Client;Formatter;</PackageTags>
        <Description>The formatter for Zaaby http client.</Description>
        <PackageProjectUrl>https://github.com/PicoHex/Zaaby</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Zaabee.Serializer.Abstractions" Version="2022.14.0" />
        <PackageReference Include="Zaaby.Shared" Version="2022.2.0" />
    </ItemGroup>

</Project>
