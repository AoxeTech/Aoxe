<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;net6.0;net7.0</TargetFrameworks>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageVersion>2023.1.0</PackageVersion>
		<Version>2023.1.0</Version>
		<Authors>Mutuduxf</Authors>
		<Company>Mutuduxf</Company>
		<PackageTags>Aoxe;MicroService;Http Client;Formatter;Protobuf;</PackageTags>
		<Description>The protobuf formatter for Aoxe http client.</Description>
		<PackageProjectUrl>https://github.com/AoxeTech/Aoxe</PackageProjectUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Aoxe.Protobuf" Version="2023.6.0" />
		<PackageReference Include="Aoxe.Client.Http.Formatter" Version="2022.9.0" />
	</ItemGroup>
</Project>