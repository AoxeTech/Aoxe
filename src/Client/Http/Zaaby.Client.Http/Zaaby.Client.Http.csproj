<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>2022.2.2</PackageVersion>
        <Version>2022.2.2</Version>
        <Authors>Mutuduxf</Authors>
        <Company>Mutuduxf</Company>
        <PackageTags>Zaaby;MicroService;Http Client</PackageTags>
        <Description>Client for Zaaby framework base by http.</Description>
        <PackageProjectUrl>https://github.com/PicoHex/Zaaby</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.Extensions.Http" Version="6.0.0" />
        <PackageReference Include="Zaabee.NewtonsoftJson" Version="2022.16.0" />
        <PackageReference Include="Zaaby.Client.Http.Formatter" Version="2022.2.0" />
        <PackageReference Include="Zaaby.Shared" Version="2022.2.0" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\Formatter\Zaaby.Client.Http.Formatter\Zaaby.Client.Http.Formatter.csproj" />
    </ItemGroup>

</Project>
