<Project Sdk="Microsoft.NET.Sdk.Web">
	<PropertyGroup>
		<TargetFrameworks>netcoreapp3.1;net6.0</TargetFrameworks>
		<OutputType>Library</OutputType>
		<LangVersion>latest</LangVersion>
		<IsPackable>true</IsPackable>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageVersion>2022.10.0.6</PackageVersion>
		<Version>2022.10.0.6</Version>
		<Authors>Mutuduxf</Authors>
		<Company>Mutuduxf</Company>
		<PackageTags>Zaaby;asp.net core formatters;protobuf</PackageTags>
		<PackageProjectUrl>https://github.com/PicoHex/Zaaby/tree/master/src/Server/Formatters/Zaaby.AspNetCore.Formatters</PackageProjectUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Description>Protobuf formatters for asp.net core</Description>
		<LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Zaabee.Protobuf" Version="2022.16.4.4"/>
		<PackageReference Include="Zaaby.AspNetCore.Formatters" Version="2022.10.2"/>
	</ItemGroup>
</Project>