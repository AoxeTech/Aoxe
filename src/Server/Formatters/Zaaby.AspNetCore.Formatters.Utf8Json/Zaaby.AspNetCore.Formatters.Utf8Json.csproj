<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFrameworks>net6.0;net7.0</TargetFrameworks>
        <OutputType>Library</OutputType>
        <LangVersion>latest</LangVersion>
        <IsPackable>true</IsPackable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>2023.1.0</PackageVersion>
        <Version>2023.1.0</Version>
        <Authors>Mutuduxf</Authors>
        <Company>Mutuduxf</Company>
        <PackageTags>Zaaby;asp.net core formatters;Utf8Json</PackageTags>
        <PackageProjectUrl>https://github.com/PicoHex/Zaaby/tree/master/src/Server/Formatters/Zaaby.AspNetCore.Formatters</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <Description>Utf8Json formatters for asp.net core</Description>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Zaabee.Utf8Json" Version="2023.6.0" />
        <PackageReference Include="Zaaby.AspNetCore.Formatters" Version="2023.1.0" />
    </ItemGroup>

</Project>
