<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFrameworks>netcoreapp3.1;net6.0</TargetFrameworks>
        <OutputType>Library</OutputType>
        <LangVersion>latest</LangVersion>
        <IsPackable>true</IsPackable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>2022.10.0.2</PackageVersion>
        <Version>2022.10.0.2</Version>
        <Authors>Mutuduxf</Authors>
        <Company>Mutuduxf</Company>
        <PackageTags>Zaaby;asp.net core formatters;ZeroFormatter</PackageTags>
        <PackageProjectUrl>https://github.com/PicoHex/Zaaby/tree/master/src/Server/Formatters/Zaaby.AspNetCore.Formatters</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <Description>Formatters for asp.net core base by ZeroFormatter.</Description>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Zaabee.ZeroFormatter" Version="2022.16.2.2" />
        <PackageReference Include="Zaaby.AspNetCore.Formatters" Version="2022.10.2" />
    </ItemGroup>

</Project>
