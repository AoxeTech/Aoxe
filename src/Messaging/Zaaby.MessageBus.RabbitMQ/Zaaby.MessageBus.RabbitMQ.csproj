<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;net6.0;net7.0</TargetFrameworks>
		<LangVersion>latest</LangVersion>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageVersion>0.10.10</PackageVersion>
		<Version>0.10.10</Version>
		<Authors>Mutuduxf</Authors>
		<Company>Mutuduxf</Company>
		<PackageTags>Zaaby;MicroService;Message bus</PackageTags>
		<Description>Message hub for Zaaby framework by RabbitMQ.</Description>
		<PackageProjectUrl>https://github.com/PicoHex/Zaaby</PackageProjectUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="7.0.0" />
		<PackageReference Include="Zaabee.RabbitMQ.Abstractions" Version="2023.11.0" />
		<PackageReference Include="Zaaby.Shared" Version="2022.3.0" />
	</ItemGroup>
</Project>