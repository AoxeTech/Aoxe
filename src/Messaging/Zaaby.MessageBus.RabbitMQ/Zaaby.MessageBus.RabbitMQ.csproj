<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
		<LangVersion>latest</LangVersion>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageVersion>0.10.8</PackageVersion>
		<Version>0.10.8</Version>
		<Authors>Mutuduxf</Authors>
		<Company>Mutuduxf</Company>
		<PackageTags>Zaaby;MicroService;Message bus</PackageTags>
		<Description>Message hub for Zaaby framework by RabbitMQ.</Description>
		<PackageProjectUrl>https://github.com/PicoHex/Zaaby</PackageProjectUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="6.0.0"/>
		<PackageReference Include="Zaabee.RabbitMQ.Abstractions" Version="2022.13.0"/>
		<PackageReference Include="Zaaby.Shared" Version="2022.2.2"/>
	</ItemGroup>
</Project>