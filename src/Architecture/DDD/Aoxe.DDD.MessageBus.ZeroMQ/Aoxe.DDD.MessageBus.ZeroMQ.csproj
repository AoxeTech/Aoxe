<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>netstandard2.0;net6.0;net7.0</TargetFrameworks>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>2022.3.0</PackageVersion>
        <Version>2022.3.0</Version>
        <Authors>Mutuduxf</Authors>
        <Company>Mutuduxf</Company>
        <PackageTags>Aoxe Message Bus ZeroMQ</PackageTags>
        <Description>Message bus base by ZeroMQ</Description>
        <PackageProjectUrl>https://github.com/PicoHex/Aoxe</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Aoxe.ZeroMQ.Abstractions" Version="2023.4.0" />
        <PackageReference Include="Aoxe.DDD.Abstractions" Version="2023.1.0" />
    </ItemGroup>

</Project>
