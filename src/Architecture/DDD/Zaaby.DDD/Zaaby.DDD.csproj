<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageVersion>2022.2.4</PackageVersion>
		<Version>2022.2.4</Version>
		<Authors>Mutuduxf</Authors>
		<Company>Mutuduxf</Company>
		<PackageTags>Zaaby;DDD;Framework</PackageTags>
		<Description>DDD framework for Zaaby.</Description>
		<PackageProjectUrl>https://github.com/PicoHex/Zaaby</PackageProjectUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.EntityFrameworkCore" Version="6.0.3"/>
		<PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="6.0.0"/>
		<PackageReference Include="Microsoft.Extensions.Hosting" Version="6.0.1"/>
		<PackageReference Include="Zaabee.SequentialGuid" Version="2022.4.0"/>
		<PackageReference Include="Zaabee.Serializer.Abstractions" Version="2022.14.0"/>
		<PackageReference Include="Zaaby.Shared" Version="2022.2.0"/>
		<PackageReference Include="Zaaby.DDD.Abstractions" Version="2022.2.0"/>
	</ItemGroup>
</Project>