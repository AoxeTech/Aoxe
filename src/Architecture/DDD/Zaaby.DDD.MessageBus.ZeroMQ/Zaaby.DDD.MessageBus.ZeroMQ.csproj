<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>2022.2.0.2</PackageVersion>
        <Version>2022.2.0.2</Version>
        <Authors>Mutuduxf</Authors>
        <Company>Mutuduxf</Company>
        <PackageTags>Zaaby Message Bus ZeroMQ</PackageTags>
        <Description>Message bus base by ZeroMQ</Description>
        <PackageProjectUrl>https://github.com/PicoHex/Zaaby</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Zaabee.ZeroMQ.Abstractions" Version="2022.8.0" />
        <PackageReference Include="Zaaby.DDD.Abstractions" Version="2022.2.0.2" />
    </ItemGroup>

</Project>
