<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>netstandard2.0;net6.0;net7.0</TargetFrameworks>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>2022.3.0</PackageVersion>
        <Version>2022.3.0</Version>
        <Authors>Mutuduxf</Authors>
        <Company>Mutuduxf</Company>
        <PackageTags>Aoxe;ThreeTier</PackageTags>
        <Description>Three tier framework for Aoxe.</Description>
        <PackageProjectUrl>https://github.com/PicoHex/Aoxe</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Aoxe.ThreeTier.Annotations" Version="2022.3.0" />
        <PackageReference Include="Aoxe.Shared" Version="2022.3.0" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\Aoxe.ThreeTier.Annotations\Aoxe.ThreeTier.Annotations.csproj" />
    </ItemGroup>

</Project>
